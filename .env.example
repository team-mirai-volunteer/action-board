# Update these with your Supabase details from your project settings > API
# https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=service-role-key
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
NEXT_PUBLIC_APP_ORIGIN=http://localhost:3000

SITE_URL=http://localhost:3000
ADDITIONAL_REDIRECT_URLS=http://localhost:3000

# 開発時は空でもかまいません
NEXT_PUBLIC_SENTRY_DSN=your-project-dsn
NEXT_PUBLIC_SENTRY_ENVIRONMENT=development

ENABLE_CONFIRMATIONS=true

# ローカル開発ではSMTP_ENABLEDをfalseとすることで、Inbucketにメールが届くようになる
# 本番・STG環境ではSMTP_ENABLEDをtrueとし、それ以外の項目についても設定が必要
SMTP_ENABLED=false
SMTP_HOST=smtp.mailgun.org
SMTP_USER=example@mailgun.org
SMTP_PASS=password
SMTP_ADMIN_EMAIL=noreply@example.com
SMTP_SENDER_NAME=test_sender

# Mailgun
MAILGUN_API_KEY=mailgun-api-key
MAILGUN_DOMAIN=team-mir.ai

# バッチ処理API用の管理者キー（本番環境では強力なランダム文字列を設定）
BATCH_ADMIN_KEY=development-admin-key-2025

# LINE Login credentials (Get from LINE Developers Console)
LINE_CLIENT_SECRET=your-line-channel-secret
NEXT_PUBLIC_LINE_CLIENT_ID=your-line-channel-id

# TikTok Login credentials (Get from TikTok Developer Portal)
TIKTOK_CLIENT_SECRET=your-tiktok-client-secret
NEXT_PUBLIC_TIKTOK_CLIENT_KEY=your-tiktok-client-key

# HubSpot API連携用のアクセストークン
HUBSPOT_API_KEY=pat-hubspot-api-key
HUBSPOT_CONTACT_LIST_ID=1

# データ基盤用
BQ_USER_PASSWORD=bq-password

# 党員バッジ同期用 Google Sheets 設定
GOOGLE_SERVICE_ACCOUNT_JSON={"type":"service_account","project_id":"your-project","private_key_id":"...","private_key":"-----BEGIN PRIVATE KEY-----\\n...\\n-----END PRIVATE KEY-----\\n","client_email":"service-account@project.iam.gserviceaccount.com","client_id":"..."}
GOOGLE_PARTY_MEMBER_SPREADSHEET_ID=your-spreadsheet-id

# YouTube Data API（#チームみらい 動画取得用）
YOUTUBE_API_KEY=your-youtube-api-key

# Google/YouTube OAuth連携用（Get from Google Cloud Console）
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
